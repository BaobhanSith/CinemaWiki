<h1 style="text-align:center">User</h1>
<div class="card">
	<% if @Userz %>
		<div class="image">
	   		<img src="/user.jpg" width=100%>
		</div>
		<div class="profile">
		<h2> <%= @Userz.username %></h2>
		<h3>Created - <%= @Userz.created_at %></h3>
		<h4>Last Updated - <%= @Userz.updated_at %></h4>
		<h5>Edit Status - <%= @Userz.edit %></h5>
	</div>
</div>
<% end %>
